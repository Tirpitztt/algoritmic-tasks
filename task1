function getAdmins(arr1, arr2) {
    var arrTemp = arr1.concat(arr2);
    return arrTemp.filter(function (item) {
        return item.role === 'admin';
    }).sort(function (a, b) {
        return a.id - b.id;
    });
}
function longName(arr) {
    return arr.sort(function (a, b) {
        return b.name.split(" ")[1].length - a.name.split(" ")[1].length;
    })[0];
}
